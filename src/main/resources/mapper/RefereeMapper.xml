<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sbitech.mapper.RefereeMapper">

    <select id="exist" resultType="com.sbitech.entity.Referee">
        select *
        FROM referee
        where username = #{username}
          and password = #{password};
    </select>
    <select id="selectByUsername" resultType="com.sbitech.entity.Referee">
        select *
        from referee
        where username = #{username};
    </select>

</mapper>