<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sbitech.mapper.UserMapper">

    <select id="getUsers" resultType="com.sbitech.entity.User">
        select *
        from user;
    </select>
    <select id="isExist" resultType="com.sbitech.entity.User">
        select *
        from user
        where username = #{username}
          and password = #{password};
    </select>


</mapper>
