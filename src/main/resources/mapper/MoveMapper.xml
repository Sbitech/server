<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sbitech.mapper.MoveMapper">

    <select id="getAll" resultType="com.sbitech.entity.Move">
        select * from move;
    </select>

    <select id="getByCode" resultType="java.lang.Float">
        select score from move where code=#{code};
    </select>
</mapper>