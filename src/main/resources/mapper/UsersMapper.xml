<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sbitech.mapper.UsersMapper">

    <select id="getUsers" resultType="com.sbitech.entity.Users">
        select *
        from users;
    </select>
    <select id="isExist" resultType="com.sbitech.entity.Users">
        select *
        from users
        where username = #{username}
          and password = #{password};
    </select>


</mapper>
